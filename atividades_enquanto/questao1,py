valor = 0

while valor >= 0:

    valor = int(input("Digite um valor: "))

    if valor > 10 and valor <= 100:
        qua = valor*valor
        print(f"O quadrado do numero {valor} é: {qua}")
    
    elif valor > 5 and valor < 10 :
        cu = valor*valor*valor
        print(f"O cubo do numero {valor} é {cu} ")
    
    elif valor > 100:
        print(f"Limite excedido")